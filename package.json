{
  "name": "brightonandroid",
  "type": "module",
  "version": "1.0.0",
  "description": "Mobile automation testing using WebdriverIO, Appium, TypeScript, and Cucumber",
  "scripts": {
  "test:android": "npx wdio run wdio.conf.ts",
  "allure:generate": "allure generate allure-results --clean",
  "allure:open": "allure open",
  "test:report": "npm run test:android && allure generate allure-results --clean && allure open"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@wdio/allure-reporter": "^9.20.0",
    "@wdio/appium-service": "^9.20.0",
    "@wdio/cli": "^9.20.0",
    "@wdio/cucumber-framework": "^9.20.0",
    "@wdio/globals": "^9.17.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "@wdio/types": "^9.20.0",
    "allure-commandline": "^2.34.1",
    "appium": "^3.1.0",
    "appium-inspector-plugin": "^2025.8.2",
    "appium-uiautomator2-driver": "^6.1.0",
    "expect-webdriverio": "^5.4.3",
    "fs": "^0.0.1-security",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wdio-video-reporter": "^6.2.0",
    "webdriverio": "^9.20.0"
  },
  "json.validate.enable": false
}
